BLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.platform_seat_id = PLATFORMS_SEATS.platform_seat_id)\nLEFT JOIN CPC3.SEAT\n    USING (seat_id)\nINNER JOIN\n    CPC3.DEAL\n    ON (\n        CPC3.DEAL.deal_id = PUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.deal_id\n        AND\n        CPC3.DEAL.publisher_id = PUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.publisher_id\n        AND\n        (CPC3.DEAL.deal_id = 'spotx' OR (CPC3.DEAL.deal_demand_source_type = 2 AND CPC3.DEAL.ad_review_enabled = 1))\n        AND CPC3.DEAL.deal_id LIKE '1550%'\n    )\nLEFT JOIN CPC3.PUBLISHER_AD_REVIEW\n    ON (PUBLISHER_AD_REVIEW.platform_ad_id_hash = PUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.platform_ad_id_hash\n        AND PUBLISHER_AD_REVIEW.platform_seat_id = PUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.platform_seat_id\n        AND PUBLISHER_AD_REVIEW.publisher_id = 68800)\nLEFT JOIN CPC3.PUBLISHER_AD_FILTER_ADVERTISERS\n    ON (PUBLISHER_AD_FILTER_ADVERTISERS.affiliate_id = 68800\n    AND PUBLISHER_AD_FILTER_ADVERTISERS.advertiser_id = AD.advertiser_id)\nWHERE PUBLISHER_AD_FILTER_ADVERTISERS.created_date IS NULL\nAND NOT EXISTS (\n    SELECT 1\n    FROM CPC3.AD_ADOMAINS\n    JOIN CPC3.PUBLISHER_AD_FILTER_HOSTS\n        ON (\n        AD_ADOMAINS.adomain_hash = PUBLISHER_AD_FILTER_HOSTS.host_hash\n    )\n    WHERE \n        AD_ADOMAINS.platform_ad_id_hash = AD.platform_ad_id_hash\n        AND\n        AD_ADOMAINS.platform_seat_id = AD.platform_seat_id\n        AND\n        PUBLISHER_AD_FILTER_HOSTS.affiliate_id = 68800\n)\nAND NOT EXISTS (\n    SELECT 1\n    FROM CPC3.AD_CATEGORIES\n    JOIN CPC3.PUBLISHER_AD_FILTER_ALL_CATEGORIES_VIEW\n        ON (\n        AD_CATEGORIES.taxonomy_node_id = PUBLISHER_AD_FILTER_ALL_CATEGORIES_VIEW.taxonomy_node_id\n    )\n    WHERE\n        AD_CATEGORIES.platform_ad_id_hash = AD.platform_ad_id_hash\n        AND\n        AD_CATEGORIES.platform_seat_id = AD.platform_seat_id\n        AND\n        PUBLISHER_AD_FILTER_ALL_CATEGORIES_VIEW.affiliate_id = 68800\n)\nAND\nPUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.bids <> 0\nAND\nPUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.publisher_id = 68800\nAND\nPUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.timezone = 0\nAND\nPUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.date >= '2016-10-21'\nAND\nPUBLISHER_AD_BID_ACTIVITY_SUMMARY_DAY.date <= '2016-10-27'\nAND (PUBLISHER_AD_REVIEW.status = 0 OR PUBLISHER_AD_REVIEW.status IS NULL)\n\nGROUP BY AD.platform_ad_id_hash, AD.platform_seat_id\n\nLIMIT 500",
